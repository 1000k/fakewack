{
  "name": "fakewack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "db": "podman-compose down --volumes && podman-compose up -d",
    "db:seed": "npx tsx prisma/seeds.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint --fix",
    "fmt": "prettier --write",
    "prepare": "husky",
    "test": "true"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint",
      "npm run fmt"
    ],
    "*.{css,ts,tsx}": [
      "npm run fmt"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@google/genai": "^1.0.1",
    "@prisma/client": "6.8.2",
    "@vercel/blob": "^1.1.1",
    "mime": "^4.0.7",
    "next": "^15.3.2",
    "next-auth": "5.0.0-beta.28",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.17.50",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
